    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251121.0",
    "@eslint/js": "^9.39.1",
    "@trivago/prettier-plugin-sort-imports": "^6.0.0",
    "@types/d3": "^7.4.3",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@vitest/browser": "^4.0.13",
    "bun-types": "latest",
    "esbuild-loader": "^4.4.0",
