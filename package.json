    "happy-dom": "^20.0.10",
    "html-webpack-plugin": "^5.6.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0",
