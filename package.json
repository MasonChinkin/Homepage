{
  "name": "homepage",
  "private": true,
  "scripts": {
    "start": "webpack serve --config ./webpack.dev.ts",
    "start:functions": "bun run build && wrangler pages dev dist",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.ts",
    "analyze": "webpack --config ./webpack.analyze.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:browser": "vitest --browser=chrome ",
    "lint": "eslint --fix src",
    "format:check": "prettier --check",
    "format:fix": "prettier --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "d3": "^7.9.0",
